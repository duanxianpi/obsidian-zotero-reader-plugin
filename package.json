{
	"name": "obsidian-zotero-reader-plugin",
	"version": "1.0.5",
	"description": "A Zotero reader plugin for Obsidian",
	"main": "main.js",
	"scripts": {
		"dev:reader": "cd reader/reader && npm install && webpack --config webpack.obsidian.js --mode development --watch",
		"dev:plugin": "webpack --watch --mode=development",
		"build": "npm run build:reader && npm run build:plugin",
		"build:pdf.js": "cd reader/reader && npm run build:pdf.js",
		"build:reader": "cd reader/reader && npm install && webpack --config webpack.obsidian.js --mode production",
		"build:plugin": "tsc -noEmit -skipLibCheck && webpack --config webpack.config.js --mode production",
		"version": "node version-bump.mjs && git add manifest.json versions.json"
	},
	"keywords": [],
	"author": "",
	"license": "AGPL-3.0-only",
	"devDependencies": {
		"@types/node": "^16.11.6",
		"@types/pako": "^2.0.3",
		"@types/react": "^19.1.8",
		"@types/react-dom": "^19.1.6",
		"@types/webpack-env": "^1.18.8",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "^3.3.0",
		"esbuild": "^0.25.6",
		"obsidian": "latest",
		"tslib": "2.4.0",
		"typescript": "^4.7.4"
	},
	"dependencies": {
		"@codemirror/language": "^6.11.3",
		"css-loader": "^7.1.2",
		"mini-css-extract-plugin": "^2.9.2",
		"monkey-around": "^3.0.0",
		"pako": "^2.1.0",
		"penpal": "^7.0.4",
		"react": "^19.1.0",
		"react-dom": "^19.1.0",
		"ts-loader": "^9.5.2",
		"uuid": "^11.1.0",
		"webpack": "^5.100.2",
		"webpack-cli": "^6.0.1"
	}
}
