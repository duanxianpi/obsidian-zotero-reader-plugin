{
	"name": "obsidian-zotero-reader-plugin",
	"version": "0.1.0",
	"description": "A Zotero reader plugin for Obsidian",
	"main": "main.js",
	"scripts": {
		"dev:plugin": "node esbuild.config.mjs ",
		"dev:reader": "cd reader && sh dev-reader",
		"build": "npm run build:pdf.js && npm run build:reader",
		"build:pdf.js": "cd reader && sh build-pdfjs",
		"build:reader": "cd reader && sh build-reader",
		"build:plugin": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json"
	},
	"keywords": [],
	"author": "",
	"license": "AGPL-3.0-only",
	"devDependencies": {
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"obsidian": "latest",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	},
	"dependencies": {
		"webpack": "^5.100.0",
		"webpack-cli": "^6.0.1"
	}
}
